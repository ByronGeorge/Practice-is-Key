#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int size;
    scanf("%d", &size);
    int arr[size];
    int number;
    
    for(int i = 0; i < size; i++){
        scanf("%d", &number);
        arr[i] = number;
    }
    for(int i = size - 1; i >= 0; i--){
        cout << arr[i] << " ";
    }
    return 0;
}
